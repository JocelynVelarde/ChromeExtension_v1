{
    "manifest_version": 3,
    "name": "My First Extension",
    "version": "1.0",
    "description": "This is a basic extension",
    "icons": {
      "16": "/images/icon_16.png",
      "48": "/images/icon_48.png",
      "128": "/images/icon_128.png"
    },
    "action": {
      "default_icon": {
        "16": "/images/icon_16.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
      },
      "default_popup": "popup.html"
    },
    "permissions": [
      "https://www.linkedin.com/*",
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www.linkedin.com/*"
        ],
        "js": ["content_script.js"]
      }
    ]
  }
  